        "@sveltejs/vite-plugin-svelte": "^6.0.0",
        "@tsconfig/svelte": "^5.0.4",
        "@types/qrcode": "^1.5.5",
        "autoprefixer": "^10.4.21",
        "cross-env": "^10.0.0",
        "cssnano": "^7.1.0",
        "postcss": "^8.5.6",
        "postcss-extend-rule": "^4.0.0",
        "postcss-load-config": "^6.0.1",
        "qrcode": "^1.5.4",
        "svelte": "^5.35.5",
